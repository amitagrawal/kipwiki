%head
  %meta{:content => "text/html; charset=utf-8", "http-equiv" => "Content-Type"}/
  %title
  %link{:href => "css/global.css", :rel => "stylesheet", :type => "text/css"}/
  %link{:href => "css/project_edit.css", :rel => "stylesheet", :type => "text/css"}/
%script{:src => "js/jquery-1.4.2.min.js", :type => "text/javascript"}
%script{:src => "js/jquery-ui-1.8.6.custom.min.js", :type => "text/javascript"}
%script{:src => "js/corners.js", :type => "text/javascript"}
%script{:src => "js/javascript.js", :type => "text/javascript"}   
#content
  / div right start
  .right
    %h1.heading Add/Edit Contents
    .borderBottom
    %h1.marTop20 Contents
    %ul.leftNavigation
      %li.main
        = link_to "Project Outline" , "#" 
        %ul.leftSub
          %li
            = link_to "Summary","#", :class => "eidtArow" 
          %li
            = link_to "Objectives", "#", :class => "okArow"
          %li
            = link_to "Key stakeholders" , "#"
      %li
        = link_to "Funding", "#"
      %li
        = link_to "Similar Projects", "#"
      %li
        = link_to "Links", "#"
      %li
        = link_to "Comments", "#"
    .addContent
      = link_to "Add new content page:", "#"
    .borderBottom
  / div right end
  / div left start
  .left
    %h1.heading Summary
    .borderBottom
    %p Describe the reason for the project. It might be a problem to be solved or an opportunity for something new
    %form{:action => "#", :method => "post"}
      .textediter
      %input.submitbg{:type => "button", :value => "Save Summary"}/
  / div left End
  .clr
/ content end
.clr
/ footer Start
#footer
/ footer end
.clr
